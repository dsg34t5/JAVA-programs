import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter a number: ");
        int num = sc.nextInt();
        int rem=0;
        if(num!=0){
            num=num%10;
            rem=rem+num; 
        }
        System.out.println("the last digit is "+ rem);
    }
}
